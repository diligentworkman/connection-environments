{
  "name": "connection-environments",
  "version": "0.0.0",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "description": "Connection environments public API for building extensions",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "prepublishOnly": "pnpm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "check-types": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diligentworkman/connection-environments.git"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "bugs": {
    "url": "https://github.com/diligentworkman/connection-environments/issues"
  },
  "homepage": "https://github.com/diligentworkman/connection-environments#readme",
  "keywords": [
    "connection-environments",
    "ui",
    "frontend",
    "typescript"
  ],
  "devDependencies": {
    "@repo/tsconfig": "workspace:*",
    "@vitest/ui": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
